	<parameter name="identificacion" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT  
p.tipo,
p.descripcion,
e.nombre as estado,
p.tiempo_evolucion
        
FROM	usuario u	

LEFT JOIN   admision ad on ad.id_usuario = u.id_usuario
LEFT JOIN   historia h on h.id_admision = ad.id_admision
LEFT JOIN   patologico p on p.id_historia = h.id_historia
LEFT JOIN   estado e on e.id_estado = p.id_estado

WHERE   ad.id_admision = UNHEX($P{identificacion})
ORDER BY p.tipo DESC]]>
	</queryString>
